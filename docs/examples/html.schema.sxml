(schema @start html
   (def-attr :xmlns :text)
   (def-attr :xml:lang :text)
   (def-attr :lang :text)
   (def-attr :type :text)
   (def-el :html (? :xmlns :xml:lang :lang :type) (? id) (head :body))
   (def-el :head :title)
   (def-el :body (* :p :h1))
   (def-attr :id (* :text))
   (def-el :h1 (? :id) :text)
   (def-el :p :text)
   (def-el :title :text)
)